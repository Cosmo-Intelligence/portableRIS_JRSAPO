<?xml version="1.0" encoding="UTF-8"?>
<Settings>

	<!-- アプリケーションID -->
	<!--
	  説明：チェックアウト時の、アプリケーションIDとして使用しています。
	        ※変更しないで下さい。
	-->
	<AppId>P_RIS</AppId>

	<!-- 検査種別ID：ポータブル -->
	<!--
	  説明：ポータブルに該当する、検査種別を設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<KensaTypeId>10,15,20,30,50</KensaTypeId>

	<!-- 検査種別ID：参照モード -->
	<!--
	  説明：参照モードに該当する、検査種別を設定して下さい。 ※カンマ区切りで複数設定可
	-->
<!-- 	<KensaTypeId_Read>CH,PO</KensaTypeId_Read> -->
	<KensaTypeId_Read>10,15,20,30,50</KensaTypeId_Read>

	<!-- 職員区分：検査技師 -->
	<!--
	  説明：オーダ一覧画面の受付者コンボボックスに表示する職員の職員区分を設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<SyokuinKbn>3</SyokuinKbn>

	<!-- 検査ステータス：未受,受済,保留,検中,検済 -->
	<!--
	  説明：オーダ一覧画面の検査ステータスボタン、ならびにオーダ検索結果一覧ステータスの名称・色情報を取得する際に使用します。 ※カンマ区切りで複数設定可
	-->
	<StatusCode>0,2,10,20,21,90</StatusCode>

	<!-- 検査ステータス変換 -->
	<!--
	  説明：オーダ一覧画面の検査ステータスボタンを押下し、オーダ検索する際の検査ステータス条件として、使用します。 ※カンマ区切りで複数設定可
	        例) 検査ステータス変換：未受 ⇒ 0,1,2を設定
	            オーダ一覧画面の検査ステータスボタン「未受」を押下 ⇒ STATUS in (0, 1, 2)で検索を行う。
	-->
	<!-- 検査ステータス変換：未受 -->
	<StatusCode_MIUKE>0,1</StatusCode_MIUKE>

	<!-- 検査ステータス変換：呼出 -->
	<StatusCode_YOBIDASI>2</StatusCode_YOBIDASI>

	<!-- 検査ステータス変換：受済,検中 -->
	<StatusCode_UKEZUMI>10</StatusCode_UKEZUMI>

	<!-- 検査ステータス変換：検中 -->
	<StatusCode_KENTYUU>20</StatusCode_KENTYUU>

	<!-- 検査ステータス変換：保留 -->
	<StatusCode_HORYUU>21,22</StatusCode_HORYUU>

	<!-- 検査ステータス変換：検済 -->
	<StatusCode_KENZUMI>90</StatusCode_KENZUMI>

	<!-- テンプレートグループ -->
	<!--
	  説明：オーダ詳細画面の連絡メモテンプレートコンボボックスに表示するテンプレートグループを設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<GroupCode>2001,2002,2003</GroupCode>

	<!-- 病棟ID -->
	<!--
	  説明：オーダ一覧画面の病棟コンボボックスに表示する病棟IDを設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<ByoutouId>106,107,108,109,110,111,112,208,209,210,211,212,213,307,308,309,310,311,312,313,314,315</ByoutouId>

	<!-- 入外区分 ※ID:名称 -->
	<!--
	  説明：オーダ一覧画面の入外コンボボックスに表示する入外区分を設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<NyugaiKbn>1:外来,2:入院</NyugaiKbn>

	<!-- ポータブル区分 ※ID:名称 -->
	<!--
	  説明：オーダ一覧画面のポータブルコンボボックスに表示するポータブル区分を設定して下さい。 ※カンマ区切りで複数設定可
	-->
	<PortableKbn>1:ﾎﾟｰﾀﾌﾞﾙ,2:手術室</PortableKbn>

	<!-- 依頼コメント -->
	<!--
	  説明：オーダ詳細画面依頼情報の依頼コメントヘッダー文字列を設定して下さい。
	-->
	<ORDERCOMMENT_ID>【依頼コメント】</ORDERCOMMENT_ID>

	<!-- 検査目的 -->
	<!--
	  説明：オーダ詳細画面依頼情報の検査目的ヘッダー文字列を設定して下さい。
	-->
	<KENSA_SIJI>【検査目的】</KENSA_SIJI>

	<!-- 臨床診断 -->
	<!--
	  説明：オーダ詳細画面依頼情報の臨床診断ヘッダー文字列を設定して下さい。
	-->
	<RINSYOU>【臨床診断】</RINSYOU>

	<!-- 備考 -->
	<!--
	  説明：オーダ詳細画面依頼情報の備考ヘッダー文字列を設定して下さい。
	-->
	<REMARKS>【備考】</REMARKS>

	<!-- 障害情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報の障害情報ヘッダー文字列を設定して下さい。
	-->
	<HANDICAPPED>【障害情報】</HANDICAPPED>

	<!-- 感染情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報の感染情報ヘッダー文字列を設定して下さい。
	-->
	<INFECTION>【感染情報】</INFECTION>

	<!-- 禁忌情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報の禁忌情報ヘッダー文字列を設定して下さい。
	-->
	<CONTRAINDICATION>【禁忌情報】</CONTRAINDICATION>

	<!-- ｱﾚﾙｷﾞｰ情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報のｱﾚﾙｷﾞｰ情報ヘッダー文字列を設定して下さい。
	-->
	<ALLERGY>【ｱﾚﾙｷﾞｰ情報】</ALLERGY>

	<!-- 妊娠情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報の妊娠情報ヘッダー文字列を設定して下さい。
	-->
	<PREGNANCY>【妊娠情報】</PREGNANCY>

	<!-- その他情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報のその他情報ヘッダー文字列を設定して下さい。
	-->
	<NOTES>【その他情報】</NOTES>

	<!-- 検査データ情報 -->
	<!--
	  説明：オーダ詳細画面患者プロファイル情報の検査データ情報ヘッダー文字列を設定して下さい。
	-->
	<EXAMDATA>【検査データ情報】</EXAMDATA>

	<!-- 部位コメント -->
	<!--
	  説明：オーダ詳細画面撮影部位の部位コメントヘッダー文字列を設定して下さい。
	-->
	<BUICOMMENT>     【コメント】</BUICOMMENT>

	<!-- MWMソケット通信タイムアウト（秒） -->
	<!--
	  説明：MWMソケット通信タイムアウト（秒）を設定して下さい。
	-->
	<MWMTimeout>60</MWMTimeout>

	<!-- MPPS取得タイムアウト（秒） -->
	<!--
	  説明：MPPS取得タイムアウト（秒）を設定して下さい。
	-->
	<MPPSTimeout>60</MPPSTimeout>

	<!-- かなローマ変換設定テキストファイルパス -->
	<!--
	  説明：患者氏名カナをローマ字変換する際に、使用します。
	        ※変更しないで下さい。
	-->
	<KanaRomaText>C:/Program Files/Apache Software Foundation/Tomcat 6.0/webapps/PortableRIS/WEB-INF/KanaRomaList.txt</KanaRomaText>

	<!-- MPPS使用フラグ -->
	<!--
	  説明：詳細画面でMPPSチェックボックスを表示する場合trueを設定してください。
	 -->
	<UseMPPS>true</UseMPPS>

	<!-- ポータブル区分使用フラグ -->
	<!--
	  説明：一覧画面でポータブル区分コンボボックスを表示する場合trueを設定してください。
	 -->
	<UsePortableKbn>true</UsePortableKbn>

	<!-- 受付時MWM登録使用フラグ -->
	<!--
	  説明：一覧画面で受付時MWM登録チェックボックスを表示する場合trueを設定してください。
	 -->
	<UseReceiptMWM>true</UseReceiptMWM>

	<!-- 排他使用フラグ -->
	<!--
	  説明：一覧画面で排他制御をする場合trueを設定してください。
	 -->
	<Exclusive>true</Exclusive>

	<!-- 排他IPフラグ -->
	<!--
	  説明：排他制御をする場合に実IPを使う場合trueを設定してください。falseの場合はログイン名が使用されます。
	 -->
	<UsePIP>true</UsePIP>

	<!-- 受付ボタン押下後、撮影詳細画面に遷移するフラグ  -->
	<!--
	  説明：
	     受付ボタン押下後、患者の撮影詳細画面に遷移する場合trueを設定してください。
	     falseの場合は、受付ボタン押下後、患者の撮影詳細画面に遷移しません。
	 -->
	<TransitionFlg>true</TransitionFlg>

	<!-- 検査進捗ボタンフラグ(色設定) -->
	<!--
	  説明：
	     設定ファイルで検査進捗ボタンの色を設定する場合はtrueを設定してください。
	     falseの場合は、DBのSTATUSDEFINEテーブルから色を取得します。
	     ※falseの場合は、ボタンのON/OFFの色分けは出来ません。
	 -->
	<StatusColorFlg>true</StatusColorFlg>

	<!-- 検査進捗ボタンの文字色(ON) -->
	<!--
	  説明：検査進捗ボタンがONの時の文字色を設定してください。
	  ※設定する色は10進数。
	 -->
	<StatusColor_ON>255</StatusColor_ON>

	<!-- 検査進捗ボタンの背景色(ON) -->
	<!--
	  説明：検査進捗ボタンがONの時の背景色を設定してください。
	  ※設定する色は10進数。
	 -->
	<StatusColorBk_ON>14739198</StatusColorBk_ON>

	<!-- 検査進捗ボタンの文字色(OFF) -->
	<!--
	  説明：検査進捗ボタンがOFFの時の文字色を設定してください。
	  ※設定する色は10進数。
	 -->
	<StatusColor_OFF>0</StatusColor_OFF>

	<!-- 検査進捗ボタンの背景色(OFF) -->
	<!--
	  説明：検査進捗ボタンがOFFの時の背景色を設定してください。
	  ※設定する色は10進数。
	 -->
	<StatusColorBk_OFF>12632256</StatusColorBk_OFF>


	<!-- ViewAirを起動するためのURL設定 -->
	<!-- 【使用出来るパラメータ】 -->
	<!-- %USER_ID% ： ユーザID -->
	<!-- %USER_PW% ： パスワード -->
	<!-- %AC_NO% ： アクセッション番号 -->
	<!-- 【注意事項】 -->
	<!-- 「&」の記述は「&amp;」として下さい。 -->
	<!-- 設定が空 または 設定が存在しない場合、ViewAirボタンは表示されません。 -->
	<!-- 【設定例】 -->
	<!-- http://10.10.81.179:8080/launch?action=view&amp;Client=mobile&amp;username=%USER_ID%&amp;password=%USER_PW%&amp;DicomRepository=VIEWAIR&amp;AccessionNumber=%AC_NO% -->
	<ViewAirUrl>http://10.10.81.179:8080/launch?action=view&amp;Client=mobile&amp;username=%USER_ID%&amp;password=%USER_PW%&amp;DicomRepository=VIEWAIR&amp;AccessionNumber=%AC_NO%</ViewAirUrl>
</Settings>
